mvn -v


mvn archetype:generate -DgroupId=com.mycompany.app -DartifactId=my-app -DarchetypeArtifactId=maven-archetype-quickstart -DarchetypeVersion=1.4 -DinteractiveMode=false


mvn archetype:generate -DgroupId=com.lti.cld -DartifactId=OnlineExam -DarchetypeArtifactId=maven-archetype-quickstart -DarchetypeVersion=1.4  -DinteractiveMode=false


mvn package



mvn exec:java -Dexec.mainClass=com.lti.clg.App


mvn site




java -jar jenkins.war --httpPort-9191




